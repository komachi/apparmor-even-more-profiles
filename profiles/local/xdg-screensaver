  include <abstractions/user-tmp>
  /{usr/,}bin/ln rix,
  /{usr/,}bin/sleep rix,
  /{usr/,}bin/rm rix,
  /{usr/,}bin/mktemp rix,
  /{usr/,}bin/cut rix,
  /{usr/,}bin/ps rix,
  owner /dev/tty[0-9] rw,
  owner @{PROC}/@{pid}/stat r,
  @{PROC}/sys/kernel/osrelease r,
  @{PROC}/uptime r,
  @{PROC}/sys/kernel/pid_max r,
  @{PROC}/ r,
